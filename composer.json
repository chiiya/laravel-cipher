{
    "name": "chiiya/laravel-cipher",
    "description": "Searchable and secure encryption for Laravel based on ciphersweet",
    "keywords": [
        "laravel",
        "ciphersweet",
        "searchable-encryption",
        "encryption"
    ],
    "homepage": "https://github.com/chiiya/laravel-cipher",
    "license": "MIT",
    "authors": [
        {
            "name": "Elisha Witte",
            "email": "elishawitte@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/contracts": "^8.37",
        "paragonie/ciphersweet": "^3.0",
        "spatie/laravel-package-tools": "^1.4.3"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "Chiiya\\LaravelCipher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chiiya\\LaravelCipher\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Chiiya\\LaravelCipher\\LaravelCipherServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
